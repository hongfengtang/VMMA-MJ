<?xml version="1.0" encoding="UTF-8"?>
<config>
    <input>
        <jar in="MMH-VMMA-1.0.jar" out="obf-MMH-VMMA-1.0.jar"/>
    </input>
    <watermark key="secure-key-to-extract-watermark" value="developer: HF"/>
    <keep-names>
        <class access="protected+">
            <field access="protected+"/>
            <method access="protected+"/>
        </class>
        <class template="class * instanceof java.io.Serializable"/>
        <class template="class * instanceof java.io.Serializable">
            <field template="static final long serialVersionUID"/>
            <method template="void writeObject(java.io.ObjectOutputStream)"/>
            <method template="void readObject(java.io.ObjectInputStream)"/>
            <method template="java.lang.Object writeReplace()"/>
            <method template="java.lang.Object readResolve()"/>
        </class>

    </keep-names>
    <ignore-classes>
        <class template="class *springframework*"/>
<!-- 
        <class template="class *shardingjdbc"/>
        <class template="class *jni*"/>
        <class template="class *apache*"/>
        <class template="class *org*"/>
        <class template="class *sun*"/>
-->
        <class template="class com.mmh.vmma.VmmaApplication"/>
        <class template="class com.mmh.vmma.hibernate.**"/>
        <class template="class com.mmh.vmma.jpa.**"/>
<!-- 
        <class template="class com.mmh.vmma.common.**"/>
        <class template="class com.mmh.vmma.ui.**"/>
        <class template="class com.mmh.vmma.utils.*"/>
-->
    </ignore-classes>
    <classpath>
    	<jar name="C:/Users/hongftan/.m2/repository/**/*.jar"/>
    </classpath>
</config>